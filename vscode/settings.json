{
    "editor.rulers": [80],
    "files.associations": {
        "*.dk": "dockerfile",
        "*.mk": "makefile"
    },
    "git.autofetch": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Nord",
    "python.formatting.provider": "autopep8",
    "go.useLanguageServer": true,
    "go.alternateTools": {
        "go-langserver": "gopls"
    },
    "gopls": {
        "staticcheck": true
    },
    "go.languageServerExperimentalFeatures": {
            "diagnostics": true, // for build and vet errors as you type
    },
    "[python]": {
        "editor.formatOnSave": true,
        "editor.rulers": [ 80 ],
    },
    "[go]": {
        "editor.snippetSuggestions": "none",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "[rust]":{
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "files.eol": "\n",
    "editor.fontFamily": "\"Fira Code\", Menlo, \"DejaVu Sans Mono\", Consolas, \"Lucida Console\", monospace",
    "editor.fontLigatures": true,
    "editor.renderWhitespace": "boundary",
    "editor.minimap.enabled": false,
    "window.zoomLevel": 0,
    "vs-kubernetes": {
        "vs-kubernetes.helm-path": "/Users/jan/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.draft-path": "/Users/jan/.vs-kubernetes/tools/draft/darwin-amd64/draft",
        "vs-kubernetes.minikube-path": "/Users/jan/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vs-kubernetes.minikube-path.mac": "/Users/jan/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vscode-kubernetes.minikube-path.mac": "/Users/jan/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "liveshare.audio.joinCallBehavior": "accept",
    "editor.suggestSelection": "first",
    "kotlin.languageServer.path": "/usr/local/bin/kotlin-language-server",
    "python.languageServer": "Pylance",
    "editor.accessibilitySupport": "off",
    "jupyter.askForKernelRestart": false,
    "python.showStartPage": false,
    "liveshare.authenticationProvider": "GitHub",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "python.defaultInterpreterPath": "/usr/local/opt/python@3.9/bin/python3.9",
    "rust-client.engine": "rust-analyzer"
}